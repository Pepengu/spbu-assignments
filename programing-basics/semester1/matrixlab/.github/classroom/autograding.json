{
  "tests": [
    {
      "name": "Test 0. Build",
      "setup": "g++ ./matrixlab.cpp -o matrixlab -pedantic-errors",
      "run": "! (grep -qwRE --include=*.{cc,cpp,h} '<vector>|<list>|<deque>|<set>|<map>|<string>|<algorithm>|<stack>|<queue>' ./* ) &&  (echo \"Build - OK\" )  || ( echo \"Forbiden STL containers\" && exit 9 )",
      "input": "",
      "output": "",
      "comparison": "include",
      "timeout": 1,
      "points": 0
    },
    {
      "name": "Test 1",
      "setup": "",
      "run": "./matrixlab -det tests/in/1matrix.txt output.txt && numdiff -a 1.0e-6 -q output.txt tests/out/1det.txt",
